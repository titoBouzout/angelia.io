{
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"parser": "babel-eslint",
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true,
			"impliedStrict": true,
			"es6": true
		},
		"es6": true,
		"ecmaVersion": 6,
		"sourceType": "module",
		"allowImportExportEverywhere": false,
		"codeFrame": false
	},
	"plugins": ["react"],
	"rules": {
		"comma-dangle": "off",
		"indent": "off",
		"linebreak-style": ["error", "unix"],
		"no-cond-assign": "off",
		"no-console": "off",
		"no-empty": "off",
		"no-extra-boolean-cast": "off",
		"no-extra-semi": "off",
		"no-fallthrough": "off",
		"no-func-assign": "off",
		"no-inner-declarations": "off",
		"no-mixed-spaces-and-tabs": "off",
		"no-redeclare": "off",
		"no-tabs": "off",
		"no-undef": "off",
		"no-unreachable": "off",
		"no-unused-vars": "off",
		"strict": "off"
	}
}
