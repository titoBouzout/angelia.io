{
	"name": "angelia.io",
	"version": "2.4.71",
	"description": "WebSockets Server and Client API for node.js and the browser, with rooms support.",
	"homepage": "https://github.com/titoBouzout/angelia.io",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/titoBouzout/angelia.io"
	},
	"main": "index.js",
	"keywords": [
		"Client",
		"Server",
		"Socket",
		"Sockets",
		"WebSocket",
		"WebSocketClient",
		"WebSocketServer",
		"events",
		"rooms api",
		"rooms",
		"socket api",
		"sockets api",
		"io",
		"ws"
	],
	"author": "@titoBouzout @boredofnames",
	"license": "GPL",
	"dependencies": {
		"bufferutil": "^4.0.3",
		"is-class": "^0.0.9",
		"isobject": "^4.0.0",
		"to-fast-properties": "^3.0.1",
		"utf-8-validate": "^5.0.4",
		"ws": "^7.x.x"
	},
	"prettier": {
		"printWidth": 100,
		"useTabs": true,
		"semi": false,
		"singleQuote": true,
		"jsxSingleQuote": false,
		"trailingComma": "all",
		"bracketSpacing": true,
		"jsxBracketSameLine": false,
		"arrowParens": "avoid",
		"quoteProps": "preserve",
		"proseWrap": "preserve"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"node": true,
			"es6": true
		},
		"parser": "babel-eslint",
		"extends": "eslint:recommended",
		"parserOptions": {
			"ecmaFeatures": {
				"experimentalObjectRestSpread": true,
				"jsx": true,
				"impliedStrict": true,
				"es6": true
			},
			"es6": true,
			"ecmaVersion": 6,
			"sourceType": "module",
			"allowImportExportEverywhere": false,
			"codeFrame": false
		},
		"plugins": [
			"react"
		],
		"rules": {
			"comma-dangle": "off",
			"indent": "off",
			"linebreak-style": [
				"error",
				"unix"
			],
			"no-cond-assign": "off",
			"no-console": "off",
			"no-empty": "off",
			"no-extra-boolean-cast": "off",
			"no-extra-semi": "off",
			"no-fallthrough": "off",
			"no-func-assign": "off",
			"no-inner-declarations": "off",
			"no-mixed-spaces-and-tabs": "off",
			"no-redeclare": "off",
			"no-tabs": "off",
			"no-undef": "off",
			"no-global-assign": "off",
			"no-unreachable": "off",
			"no-unused-vars": "off",
			"strict": "off"
		}
	}
}
